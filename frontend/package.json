{
    "type": "module",
    "scripts": {
        "start": "rollup -c -w --environment NODE_ENV:development ",
        "build": "rollup -c --environment NODE_ENV:production",
        "test:watch": "jest --watch --coverage",
        "test:all": "jest --silent --ci --all",
        "lint": "eslint ./src ./tests ./*.js && prettier -c ./src ./tests ./*.js",
        "fix": "eslint --fix ./src ./tests ./*.js && prettier -w ./src ./tests ./*.js",
        "prerelease": "npm run lint && npm run test:all"
    },
    "dependencies": {
        "@tauri-apps/api": "^1.3.0",
        "date-fns": "^2.30.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.0",
        "@rollup/plugin-node-resolve": "^15.0.2",
        "@rollup/plugin-replace": "^5.0.2",
        "@swc/core": "^1.3.58",
        "@swc/jest": "^0.2.26",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/svelte": "^3.2.2",
        "@testing-library/user-event": "^14.4.3",
        "eslint": "^8.40.0",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-jest-dom": "^4.0.3",
        "eslint-plugin-svelte3": "^4.0.0",
        "eslint-plugin-testing-library": "^5.11.0",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "postcss": "^8.4.23",
        "prettier": "^2.8.8",
        "prettier-plugin-svelte": "^2.10.0",
        "rollup": "^3.22.0",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-css-only": "^4.3.0",
        "rollup-plugin-delete": "^2.0.0",
        "rollup-plugin-livereload": "^2.0.5",
        "rollup-plugin-serve": "^2.0.2",
        "rollup-plugin-svelte": "^7.1.4",
        "rollup-plugin-swc3": "^0.8.1",
        "sass": "^1.62.1",
        "svelte": "^3.59.1",
        "svelte-check": "^3.3.2",
        "svelte-jester": "^2.3.2",
        "svelte-preprocess": "^5.0.3"
    }
}
