{
    "name": "tictoc",
    "version": "1.0.0",
    "author": "Dustin Holm",
    "type": "module",
    "scripts": {
        "start:base": "rollup -c -w --environment NODE_ENV:development ",
        "start:standalone": "npm run start:base -- --environment MODE:standalone",
        "start:tauri": "npm run start:base -- --environment MODE:tauri",
        "build:base": "rollup -c --environment NODE_ENV:production,MODE:tauri",
        "build:release": "npm run lint && npm run build:base",
        "lint": "eslint"
    },
    "dependencies": {
        "@tauri-apps/api": "^1.2.0",
        "svelte-spa-router": "^3.3.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^24.0.1",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-replace": "^5.0.1",
        "@swc/core": "^1.3.9",
        "eslint": "^8.34.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "rollup": "^3.2.3",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-css-only": "^4.1.1",
        "rollup-plugin-delete": "^2.0.0",
        "rollup-plugin-livereload": "^2.0.5",
        "rollup-plugin-serve": "^2.0.1",
        "rollup-plugin-svelte": "^7.1.0",
        "rollup-plugin-swc3": "^0.7.0",
        "sass": "^1.55.0",
        "svelte": "^3.52.0",
        "svelte-check": "^2.9.2",
        "svelte-preprocess": "^4.10.7"
    }
}
