{
    "name": "ticit",
    "version": "1.0.0",
    "author": "Dustin Holm",
    "type": "module",
    "scripts": {
        "start:base": "rollup -c -w --environment NODE_ENV:development ",
        "start:standalone": "npm run start:base -- --environment MODE:standalone",
        "start:tauri": "npm run start:base -- --environment MODE:tauri",
        "build": "rollup -c --environment NODE_ENV:production,MODE:tauri",
        "test:watch": "jest --watch --coverage",
        "test:all": "jest --silent --ci --all",
        "lint": "eslint",
        "prerelease": "npm run lint && npm run test:all"
    },
    "dependencies": {
        "@tauri-apps/api": "^1.2.0",
        "svelte-spa-router": "^3.3.0"
    },
    "devDependencies": {
        "@happy-dom/jest-environment": "^8.5.0",
        "@rollup/plugin-commonjs": "^24.0.1",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-replace": "^5.0.2",
        "@swc/core": "^1.3.35",
        "@swc/jest": "^0.2.24",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/svelte": "^3.2.2",
        "@testing-library/user-event": "^14.4.3",
        "eslint": "^8.34.0",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-jest-dom": "^4.0.3",
        "eslint-plugin-svelte3": "^4.0.0",
        "eslint-plugin-testing-library": "^5.10.2",
        "happy-dom": "^8.5.0",
        "jest": "^29.4.3",
        "postcss": "^8.4.21",
        "rollup": "^3.16.0",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-css-only": "^4.3.0",
        "rollup-plugin-delete": "^2.0.0",
        "rollup-plugin-livereload": "^2.0.5",
        "rollup-plugin-serve": "^2.0.2",
        "rollup-plugin-svelte": "^7.1.3",
        "rollup-plugin-swc3": "^0.7.0",
        "sass": "^1.58.2",
        "svelte": "^3.55.1",
        "svelte-check": "^2.10.3",
        "svelte-jester": "^2.3.2",
        "svelte-preprocess": "^4.10.7"
    }
}
